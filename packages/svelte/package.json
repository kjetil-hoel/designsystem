{
  "name": "@kjetil-hoel/design-system-svelte",
  "license": "MIT",
  "version": "0.0.26",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "npm run test:integration && npm run test:unit",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest"
  },
  "exports": {
    "./Button": "./dist/Button/Button.svelte",
    "./Modal": "./dist/Modal/Modal.svelte",
    "./Select": "./dist/Select/Select.svelte",
    "./Link": "./dist/Link/Link.svelte",
    "./List": "./dist/List/List.svelte",
    "./Alert": "./dist/Alert/Alert.svelte",
    "./Paragraph": "./dist/Typography/Paragraph/Paragraph.svelte",
    "./Tag": "./dist/Tag/Tag.svelte",
    "./Textfield": "./dist/Form/Textfield/Textfield.svelte",
    "./Radio": "./dist/Form/Radio/Radio.svelte",
    "./RadioGroup": "./dist/Form/Radio/RadioGroup.svelte",
    "./Switch": "./dist/Form/Switch/Switch.svelte",
    "./Spinner": "./dist/Spinner/Spinner.svelte",
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.4",
    "@sveltejs/package": "^2.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "publint": "^0.1.9",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "tslib": "^2.4.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.2",
    "vitest": "^0.32.2"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "gitHead": "231bcde4abc405d95f225e3e09b24e5d88a7d7b5",
  "dependencies": {
    "@altinn/figma-design-tokens": "^6.0.1",
    "@navikt/aksel-icons": "^5.6.0",
    "altinn-designsystem": "^4.2.0",
    "svelte-floating-ui": "^1.5.5",
    "uuid": "^9.0.1"
  }
}
